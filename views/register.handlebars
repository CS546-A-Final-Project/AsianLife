<nav>
        <a href="/">Login</a>

</nav>
<div class="container">
    <form action="/register" method="post">
        <h2>Register</h2>

        <label for="userName">User Name:</label>
        <input type="text" id="userName" name="userName" value="{{newUserPostData.userName}}" required>
        <label class="error">{{errors.userName}}</label>

        <label for="firstName">First Name:</label>
        <input type="text" id="firstName" name="firstName" value="{{newUserPostData.firstName}}" required>
        <label class="error">{{errors.firstName}}</label>

        <label for="lastName">Last Name:</label>
        <input type="text" id="lastName" name="lastName" value="{{newUserPostData.lastName}}" required>
        <label class="error">{{errors.lastName}}</label>

        <label for="email">Email:</label>
        <input type="email" id="email" name="email" value="{{newUserPostData.email}}" required>
        <label class="error">{{errors.email}}</label>

        <label for="password">Password:</label>
        <input type="password" id="password" name="password" value="{{newUserPostData.password}}" required>
        <label class="error">{{errors.password}}</label>

        <label for="isAdmin">Become Admin:</label>
        <input class="checkbox" id="isAdmin" name="isAdmin" type="checkbox" {{checked}}>

        <button type="submit">Register</button>
    </form>
</div>