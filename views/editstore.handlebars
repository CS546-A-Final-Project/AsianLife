<form
  enctype="multipart/form-data"
  method="post"
  action="/upload/store/{{storeId}}"
  id="uploadForm"
>
  <input type="file" name="file" id="file" accept="image/*" />
  
  <label for="store_name">Store Name:</label>
  <input
    type="text"
    id="store_name"
    name="store_name"
    value="{{store_name}}"
    required
  />
  <label for="streetAddress">Store Address:</label>
  <input
    type="text"
    id="streetAddress"
    name="streetAddress"
    value="{{streetAddress}}"
    required
  />
   <label for="city">Store city:</label>
  <input
    type="text"
    id="city"
    name="city"
    value="{{city}}"
    required
  />
   <label for="state">Store state:</label>
  <input
    type="text"
    id="state"
    name="state"
    value="{{state}}"
    required
  />
  <label for="zip">Store zip:</label>
  <input
    type="text"
    id="zip"
    name="zip"
    value="{{zip}}"
    required
  />

  <label for="email">Store Email:</label>
  <input
    type="email"
    id="email"
    name="email"
    value="{{email}}"
    required
  />
  <label for="phone">Store Phone:</label>
  <input
    type="phone"
    id="phone"
    name="phone"
    value="{{phone}}"
    required
  />
<button type="submit">Update Stroe </button>
</form>
